<h1>Sign up as a Student at MusicGuroo</h1>
<%# TODO: STYLE BETTER %>
<%= form_for @student do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username%>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <hr>
  <%= f.label :name %>
  <%= f.text_field :name%>
  <%= f.submit 'Join Now!' %>
<% end %>
